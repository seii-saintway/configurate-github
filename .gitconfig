[user]
	name = seii-saintway
	email = saintway.sakaru@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = true
	ui = true
[alias]
	co = checkout
	cm = commit
	cmm = commit -m
	cmam = commit -a -m
	st = status
	br = branch
	unstage = reset HEAD
	hist = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	type = cat-file -t
	dump = cat-file -p
[core]
	autocrlf = false
